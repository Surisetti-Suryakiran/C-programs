#include<stdio.h>
main()
{
	int n,k,i,even=0,odd=0;
	printf("enter n and k\n");
    scanf("%d %d",&n,&k);
    printf("\n1 to %d divisibles of %d are ",n,k);
    for(i=1;i<=n;i++)
    {
    	if(i%k==0)
		{
    	printf("%d ",i);
        if(i%2==0)
        even=even+i;
        else
        odd=odd+i;
		}
	}
	 printf("\nevensum : %d \noddsum : %d",even,odd);
}
